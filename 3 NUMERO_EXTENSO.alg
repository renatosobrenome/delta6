algoritmo "Número por Extenso"
var
   numero: inteiro
inicio
   escreva("Digite um número de 1 a 5: ")
   leia(numero)

   escolha numero
      caso 1: escreva("Um")
      caso 2: escreva("Dois")
      caso 3: escreva("Três")
      caso 4: escreva("Quatro")
      caso 5: escreva("Cinco")
      caso contrario:
         escreva("Número inválido.")
   fimescolha
fimalgoritmo
