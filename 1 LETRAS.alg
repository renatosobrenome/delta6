algoritmo "letras"
inicio
    caractere letra1, letra2

    escreva("Digite a primeira letra (A): ")
    leia(letra1)
    escreva("Digite a segunda letra (B): ")
    leia(letra2)

    se letra1 = "A" e letra2 = "B" então
        escreva("Você digitou as letras corretas: A e B.")
    senão
        escreva("Você digitou letras incorretas.")
    fimse
fimalgoritmo
